{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "camelcase": "off",
    "no-unused-vars": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "no-return-await": "off"

  }
}
